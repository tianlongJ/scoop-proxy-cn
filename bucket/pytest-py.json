  "hash": "8f5572e508c2cb95197e550f54c6bc03cfd4668ef3c9f1011ec3950037cb4b11",
  "installer": {
    "script": [
      "if ($global) { install-python-package --global --quiet --dir $dir --repo https:github.compytest-devpytest.git --branch $version --clone-dir $app }",
      "else { install-python-package --quiet --dir $dir --repo https:github.compytest-devpytest.git --branch $version --clone-dir $app }",
      "if ($LASTEXITCODE) { exit 1 }"
    ]
  },
  "uninstaller": {
    "script": [
      "if ($global) { uninstall-python-package $app --global --dir $dir --match '.*py.?test.*' }",
      "else { uninstall-python-package $app --dir $dir --match '.*py.?test.*' }"
    ]
  },
  "checkver": {
    "github": "https:github.compytest-devpytest"
  },
